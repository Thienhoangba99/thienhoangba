@model IEnumerable<bai5_Anh.Models.GroupImages>
@using PagedList
@using PagedList.Mvc
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NameGroup)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NameGroup)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdGroupImage }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdGroupImage }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdGroupImage })
        </td>
    </tr>
}

</table>
@Html.PagedListPager((IPagedList) Model, p => Url.Action("Index", new { page = p }))